{
  "name": "betflags",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "dependencies": {
    "linereader": "^1.0.0",
    "request": "^2.40.0",
    "line-reader": "^0.2.4"
  },
  "devDependencies": {
    "cli-progress": "^2.1.1",
    "electron": "^4.0.5",
    "electron-builder": "^20.38.5",
    "electron-packager": "^13.1.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "pack": "electron-packager . bet-steal --overwrite --platform=win32 --arch=ia32 --icon=build/steal.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Bet Steal\"",
    "dist": "build"
  },
  "build": {
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },
  "author": "",
  "license": "ISC"
}
